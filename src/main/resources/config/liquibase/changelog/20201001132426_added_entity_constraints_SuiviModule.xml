        <addForeignKeyConstraint baseColumnNames="module_id"
                                 baseTableName="suivi_module"
                                 constraintName="fk_suivi_module_module_id"
                                 referencedColumnNames="id"
                                 referencedTableName="module"/>

    </changeSet>
</databaseChangeLog>
